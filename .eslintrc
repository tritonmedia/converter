{
  "plugins": [
    "mocha"
  ],
  "parser": "babel-eslint",
  "env": {
    "mocha": true,
    "node": true,
    "es6": true
  },
  "rules": {
    "no-extra-parens": 2,
    "valid-jsdoc": 2,
    "block-scoped-var": 2,
    "mocha/no-exclusive-tests": 2,
    "complexity": [1, 9],
    "default-case": 2,
    "dot-notation": [2],
    "guard-for-in": 2,
    "no-div-regex": 2,
    "no-unused-vars": 1,
    "no-console": 0,
    "no-const-assign": 2,
    "no-else-return": 2,
    "no-eq-null": 2,
    "no-floating-decimal": 2,
    "no-process-env": 0,
    "no-self-compare": 2,
    "no-void": 2,
    "no-warning-comments": 1,
    "radix": 2,
    "vars-on-top": 2,
    "wrap-iife": 2,
    "no-catch-shadow": 2,
    "no-sync": 1,
    "no-mixed-spaces-and-tabs": 2,
    "no-underscore-dangle": 0,
    "quotes": [2, "single", "avoid-escape"],
  },
  "extends": "eslint:recommended"
}
